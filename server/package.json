    "dev:start": "bash scripts/dev/start-dev.sh",
    "fix:asu-unique": "node scripts/ensure_asu_composite_unique.js",
    "diag:check-schema": "node scripts/diagnostics/check_schema.js",
    "diag:check-yarn-col": "node scripts/diagnostics/check_yarn_type_column.js",
    "diag:find-production": "node scripts/diagnostics/find_production_data.js",
    "diag:inspect-structure": "node scripts/diagnostics/inspect_data_structure.js",
    "diag:test-asu-prod-table": "node scripts/diagnostics/test_asu_production_table.js",
    "test:routes-load": "node scripts/tests/test_routes_load.js",
    "test:api": "node scripts/tests/test_api_endpoints.js",
    "test:create-entry": "node scripts/tests/test_create_production_entry.js",
    "test:yarn-summary-u1": "node scripts/tests/test_yarn_summary_unit1.js",
    "test:duplicate": "node scripts/tests/test_duplicate_entry.js",
    "test:historical": "node scripts/tests/test_historical_preservation.js",
    "test:login-yarn": "node scripts/tests/test_login_and_yarn_type.js",
    "test:machine-config": "node scripts/tests/test_machine_config_changes.js",
    "test:machine-creation": "node scripts/tests/test_machine_creation.js",
    "test:machine-performance": "node scripts/tests/test_machine_performance_api.js",
    "test:models": "node scripts/tests/test_models.js",
    "ops:create-admin": "node scripts/ops/create-admin.js",
  "render-postbuild": "npm run migrate",
  "prestart": "npm run migrate",
  "migrate": "NODE_ENV=production npx sequelize-cli db:migrate --env production",
  "migrate:undo": "NODE_ENV=production npx sequelize-cli db:migrate:undo --env production",
  "migrate:reset": "NODE_ENV=production npx sequelize-cli db:migrate:undo:all --env production && NODE_ENV=production npx sequelize-cli db:migrate --env production",
    "postbuild": "npm run migrate",
    "sql:check-asu": "node scripts/diagnostics/check_asu_tables.js"
  ,"diag:db": "node scripts/diagnostics/test_db_connection.js"
  },
