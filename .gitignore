# .gitignore

# Node
node_modules/
.env
dist/
build/

# Logs
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# IDEs
.vscode/
.idea/

# OS
.DS_Store
Thumbs.db

# TypeScript build info
erp-frontend/tsconfig.tsbuildinfo

# Environment / secret config (keep examples)
.env.production
.env.development
.env.staging
.env.test
.env*.local
server/.env.production
server/.env.development
server/.env.staging
server/.env.test
server/.env*.local
server/.env*
*.env.backup
*.env.bak

# Never ignore example/template env files
!*.env.example
!*.example
!server/.env.example